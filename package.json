{
  "name": "discord-channel-playlist-bot",
  "description": "A Discord bot that allows users to receive a weekly Spotify playlist made up of songs posted to a specific Discord channel over that time.",
  "version": "1.0.3",
  "main": "index.js",
  "author": "Mychal Thompson",
  "license": "MIT",
  "keywords": [
    "discord",
    "discord-bot",
    "typescript",
    "spotify",
    "spotify-playlist",
    "music",
    "bot"
  ],
  "devDependencies": {
    "@types/es6-promise": "^3.3.0",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash": "^4.14.119",
    "@types/luxon": "^2.0.4",
    "@types/node": "^10.12.18",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/spotify-web-api-node": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "discord.js": "^11.4.2",
    "dotenv": "^10.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.11",
    "luxon": "^2.0.2",
    "ramda": "^0.27.1",
    "spotify-web-api-node": "^5.0.2"
  },
  "scripts": {
    "start": "nodemon --ignore ./data/ --exec ./node_modules/.bin/ts-node -r dotenv/config ./src/index.ts",
    "predebug": "tsc",
    "debug": "node inspect ./dist/index.js",
    "prepare": "husky install",
    "lint": "eslint ./src --ext .ts"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
